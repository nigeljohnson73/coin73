        "**/vendor/**": true
    },
    "intelephense.references.exclude": [
        "**/api/_include/**",
        "**/api/functions.php",
        "**/api/config*",
        "**/cron/_include/**",
        "**/cron/functions.php",
        "**/cron/config*",
